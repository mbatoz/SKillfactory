class Cat:
    def __init__(self, name, breed, age, gender):
        self.name = name
        self.breed = breed
        self. age = age
        self.gender = gender

    def getName(self):
        return self.name
    def getBreed(self):
        return self.breed
    def getAge(self):
        return self.age
    def getGender(self):
        return self.gender

    def getList(self):
        print('Имя - {}'.format(self.name))
        print('Порода - {}'.format(self.breed))
        print('Возраст - {}'.format(self.age))
        print('Пол - {}'.format(self.gender))
